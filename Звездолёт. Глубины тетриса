    cout << " \n";
    cout << "      ";
for (int j = 1; j < 6; j++)
	if (j == f) cout << " ^  \\___/  ^ ";
    else if (j == bag && gal == 1) cout << "    |   |    ";
    else if (j == pir && gal2 == 1) cout << "| |      | | ";
    else if (j == bat - 1 && gal3 == 1 && bat1 != 0 || j == bat + 1 && gal3 == 1 && bat2 != 0) cout << "      |      ";
    else cout << "             ";
    cout << " \n";
    cout << "      ";
for (int j = 1; j < 6; j++)
	if (j == f) cout << "/^\\  /^\\  /^\\";
    else if (j == bag && gal == 1) cout << "    |   |    ";
    else if (j == pir && gal2 == 1) cout << "| |      | | ";
    else if (j == bat - 1 && gal3 == 1 && bat1 != 0 || j == bat + 1 && gal3 == 1 && bat2 != 0) cout << "      |      ";
    else cout << "             ";
    cout << " \n";
    cout << "      ";
for (int j = 1; j < 6; j++)
	if (j == f) cout << "    /^^^\\    ";
    else if (j == bag && gal == 1) cout << "    |   |    ";
    else if (j == pir && gal2 == 1) cout << "| |      | | ";
    else if (j == bat - 1 && gal3 == 1 && bat1 != 0 || j == bat + 1 && gal3 == 1 && bat2 != 0) cout << "      |      ";
    else cout << "             ";
    cout << " \n";
}
void time(){//Ôóíêöèÿ íàñòðîéêè ñêîðîñòè äâèæåíèÿ è ïàðàëåëüíîãî ââîäà ñ êëàâèàòóðû
int was_hit = 0;     
clock_t ev = clock();
clock_t new_ev = 0;  
while (1) {
new_ev = clock();
if (o == 2000 || o == 7000) {
if ((new_ev - ev)/(double)CLOCKS_PER_SEC >= t2) {
break;
}
}
else if ((new_ev - ev)/(double)CLOCKS_PER_SEC >= t) {
break;
}
if (kbhit()) {
s = _getch();
was_hit = 1;
break;}}
if (was_hit == 0) s = 'w';
was_hit = 0;
new_ev = 0;
ev = 0;
}
